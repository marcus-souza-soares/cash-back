{
  "name": "ngcash-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "ttsc -p tsconfig.build.json",
    "dev": "NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node ./src/index.ts' -e ts",
    "dev:compose": "docker-compose -f docker-compose.development.yml --env-file .env.development",
    "dev:docker:down": "npm run dev:compose -- down",
    "dev:docker": "npm run dev:compose -- up --build",
    "dev:migration:dev": "npm run dev:compose -- run ngcash-development npm run dev:load-envs prisma migrate dev",
    "dev:migration:deploy": "npm run dev:compose -- run ngcash-development npm run dev:load-envs prisma migrate deploy",
    "dev:migration:generate": "npm run dev:compose -- run ngcash-development npm run dev:load-envs prisma migrate generate --schema=./schema.prisma",
    "dev:prisma:studio": "npm run dev:compose -- run ngcash-development npm run dev:load-envs prisma studio",
    "dev:seed": "npm run dev:compose -- run ngcash-development npm run dev:load-envs prisma db seed",
    "dev:load-envs": "dotenv -e .env.development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "autocannon": "^7.9.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "dotenv-expand": "^8.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^4.6.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^5.1.0",
    "dotenv-expand": "^9.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-commit-msg-linter": "^4.1.2",
    "husk": "^0.5.3",
    "jest": "^26.6.3",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^3.14.0",
    "supertest": "^6.2.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
